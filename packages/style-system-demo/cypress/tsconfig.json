{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "../../node_modules",
    "isolatedModules": false,
    "lib": [
      "es5",
      "es6",
      "es7",
      "es2017",
      "dom"
    ],
    "outDir": "build",
    "sourceMap": false,
    "types": [
      "cypress"
    ]
  },
  "include": [
    "integration/**/*.spec.ts",
    "support/**/*.ts",
    "utils/*.ts"
  ]
}

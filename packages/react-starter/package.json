{
  "name": "@indoqa/react-starter",
  "version": "1.4.0",
  "description": "Typescript based React + Redux + Redux-Observable + Formik starter",
  "license": "Apache-2.0",
  "files": [
    "src"
  ],
  "scripts": {
    "analyze": "source-map-explorer ./build/static/js/*.js{,.map}",
    "build": "indoqa-webpack-ts ./indoqa-webpack-options.js",
    "clean": "rimraf ./build",
    "lint": "eslint './src/**/*.tsx' './src/**/*.ts'",
    "package": "yarn clean && yarn lint && yarn build",
    "postinstall": "yarn run package",
    "start": "indoqa-dev-server ./indoqa-webpack-options.js",
    "test": "jest",
    "test:watch": "jest --watch --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Indoqa/indoqa-react.git"
  },
  "keywords": [
    "react",
    "react-router",
    "redux",
    "redux-observable",
    "rxjs",
    "store",
    "middleware",
    "bootstrap",
    "starter"
  ],
  "dependencies": {
    "@indoqa/react-app": "^1.4.0",
    "@indoqa/style-system": "^1.4.0",
    "fela": "^11.4.0",
    "fela-monolithic": "^11.4.0",
    "fela-plugin-named-keys": "^11.4.0",
    "fela-preset-web": "^11.4.0",
    "formik": "^2.2.1",
    "history": "^4.10.1",
    "i18next": "^19.8.3",
    "i18next-browser-languagedetector": "^6.0.1",
    "immer": "^7.0.14",
    "react": "^17.0.1",
    "react-app-polyfill": "^2.0.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^17.0.1",
    "react-fela": "^11.4.0",
    "react-i18next": "11.7.3",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.2.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.6.3",
    "shortid": "^2.2.16",
    "typesafe-actions": "^5.1.0",
    "yup": "^0.29.3"
  }
}

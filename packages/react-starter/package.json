{
  "name": "@indoqa/react-starter",
  "version": "1.0.0-alpha.6",
  "description": "Typescript based React + Redux + Redux-Observable + Formik starter",
  "license": "Apache-2.0",
  "files": [
    "src"
  ],
  "scripts": {
    "analyze": "source-map-explorer ./build/static/*.js{,.map}",
    "build": "indoqa-webpack-ts ./indoqa-webpack-options.js",
    "clean": "rimraf ./target",
    "lint": "tslint --project .",
    "package": "yarn clean && yarn build",
    "postinstall": "yarn run package",
    "start": "indoqa-dev-server ./indoqa-webpack-options.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Indoqa/indoqa-react.git"
  },
  "keywords": [
    "react",
    "react-router",
    "redux",
    "redux-observable",
    "rxjs",
    "store",
    "middleware",
    "bootstrap",
    "starter"
  ],
  "dependencies": {
    "@indoqa/react-app": "^1.0.0-alpha.6",
    "@indoqa/style-system": "^1.0.0-alpha.6",
    "fela": "^10.2.1",
    "fela-monolithic": "^10.2.1",
    "fela-plugin-named-keys": "^10.2.1",
    "fela-preset-web": "^10.2.1",
    "formik": "^1.5.1",
    "history": "^4.7.2",
    "i18next": "^15.0.6",
    "i18next-browser-languagedetector": "^3.0.1",
    "immer": "^2.1.1",
    "react": "^16.8.4",
    "react-app-polyfill": "^0.2.2",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.4",
    "react-fela": "^10.2.1",
    "react-i18next": "10.3.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.4.0",
    "shortid": "^2.2.14",
    "typesafe-actions": "^3.2.1",
    "yup": "^0.26.10"
  },
  "proxy": {
    "/geonames": {
      "target": "http://api.geonames.org",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "pathRewrite": {
        "/geonames": ""
      }
    },
    "/words": {
      "target": "http://api.wordnik.com",
      "pathRewrite": {
        "/words": ""
      }
    }
  }
}

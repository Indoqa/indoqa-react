{
  "name": "@indoqa/react-starter",
  "version": "1.3.2-rc.2",
  "description": "Typescript based React + Redux + Redux-Observable + Formik starter",
  "license": "Apache-2.0",
  "files": [
    "src"
  ],
  "scripts": {
    "analyze": "source-map-explorer ./build/static/js/*.js{,.map}",
    "build": "indoqa-webpack-ts ./indoqa-webpack-options.js",
    "clean": "rimraf ./build",
    "lint": "tslint --project .",
    "package": "yarn clean && yarn build",
    "postinstall": "yarn run package",
    "start": "indoqa-dev-server ./indoqa-webpack-options.js",
    "test": "jest",
    "test:watch": "jest --watch --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Indoqa/indoqa-react.git"
  },
  "keywords": [
    "react",
    "react-router",
    "redux",
    "redux-observable",
    "rxjs",
    "store",
    "middleware",
    "bootstrap",
    "starter"
  ],
  "dependencies": {
    "@indoqa/react-app": "^1.3.2-rc.2",
    "@indoqa/style-system": "^1.3.2-rc.2",
    "fela": "^10.7.0",
    "fela-monolithic": "^10.7.0",
    "fela-plugin-named-keys": "^10.7.0",
    "fela-preset-web": "^10.7.0",
    "formik": "^1.5.8",
    "history": "^4.10.1",
    "i18next": "^17.0.17",
    "i18next-browser-languagedetector": "^3.0.3",
    "immer": "^4.0.0",
    "react": "^16.10.1",
    "react-app-polyfill": "^1.0.3",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.10.1",

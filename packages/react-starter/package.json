{
  "name": "@indoqa/react-starter",
  "version": "1.0.0-alpha.5",
  "description": "Typescript based React + Redux + Redux-Observable + Formik starter",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:Indoqa/indoqa-react.git"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "analyze": "source-map-explorer ./target/assets/vendors-*",
    "build": "indoqa-webpack ./indoqa-webpack-options.js",
    "clean": "rimraf ./target",
    "compile": "yarn clean && tsc --outdir ./target/js",
    "lint": "tslint --project .",
    "package": "yarn clean && yarn build",
    "postinstall": "yarn run package",
    "start": "indoqa-dev-server ./indoqa-webpack-options.js"
  },
  "dependencies": {
    "@indoqa/react-app": "^1.0.0-alpha.5",
    "@indoqa/style-system": "^1.0.0-alpha.5",
    "fela": "^10.1.3",
    "fela-monolithic": "^10.1.3",
    "fela-plugin-named-keys": "^10.1.3",
    "fela-preset-web": "^10.1.3",
    "formik": "^1.4.3",
    "history": "^4.7.2",
    "i18next": "^13.0.0",
    "i18next-browser-languagedetector": "^2.2.4",
    "immer": "^1.12.1",
    "prop-types": "^15.6.2",
    "react": "16.7.0",
    "react-app-polyfill": "^0.2.1",
    "react-document-title": "^2.0.3",
    "react-dom": "16.7.0",
    "react-fela": "^10.1.3",
    "react-i18next": "8.4.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.3.3",
    "shortid": "^2.2.14",
    "typesafe-actions": "^2.0.4",
    "yup": "^0.26.10"
  },
  "proxy": {
    "/geonames": {
      "target": "http://api.geonames.org",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "pathRewrite": {
        "/geonames": ""
      }
    },
    "/words": {
      "target": "http://api.wordnik.com",
      "pathRewrite": {
        "/words": ""
      }
    }
  }
}

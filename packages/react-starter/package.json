    "react-i18next": "10.13.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.2.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.3",
    "shortid": "^2.2.15",
    "typesafe-actions": "^4.4.2",
    "yup": "^0.27.0"
